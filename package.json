{
  "name": "easy-in",
  "version": "1.0.0",
  "description": "An easier way to check-in at your favorite gym.",
  "main": "server.js",
  "scripts": {
    "start:dev": "tsx watch ./src/server.ts",
    "start": "node build/server.js",
    "build": "tsup src --out-dir build",
    "test": "vitest run --dir src/usecases",
    "test:watch": "vitest --dir src/usecases",
    "pretest:e2e": "npm link ./prisma/vitest-environment-prisma && npm link vitest-environment-prisma",
    "test:e2e": "vitest run --dir src/http",
    "test:e2e:watch": "vitest --dir src/http",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui"
  },
  "author": "brunopetrolini <brunopetrolini@hotmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@rocketseat/eslint-config": "2.1.0",
    "@types/bcryptjs": "2.4.6",
    "@types/node": "20.11.6",
    "@types/supertest": "6.0.2",
    "@vitest/coverage-v8": "1.2.2",
    "@vitest/ui": "1.2.2",
    "eslint": "8.56.0",
    "prisma": "5.8.1",
    "supertest": "6.3.4",
    "tsup": "8.0.1",
    "tsx": "4.7.0",
    "typescript": "5.3.3",
    "vite-tsconfig-paths": "4.3.1",
    "vitest": "1.2.2"
  },
  "dependencies": {
    "@fastify/jwt": "8.0.0",
    "@prisma/client": "5.8.1",
    "bcryptjs": "2.4.3",
    "dayjs": "1.11.10",
    "dotenv": "16.4.1",
    "fastify": "4.25.2",
    "zod": "3.22.4"
  }
}
